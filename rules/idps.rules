# 1010000-1049999 Reserved for IDPS Use -- Put your IDPS rules in this range to avoid conflicts

# reserved for 1010000-1010099
# disallow ping from internet
drop icmp $EXTERNAL_NET any -> $HOME_NET any (msg:"ICMP PING"; itype: 8; classtype: network-scan; priority:1; sid:1010000; rev:1;)

# Potential Ports Scan -sVC?
##drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sVC?"; flow:to_server; flags:S,12; window: 64240; tcp.mss: 1460; classtype:attempted-recon; priority:1; sid:1010020; rev:2;)
# general or -sS (SYN scan) as well as -sVC (Kali Linux host only) (filtered)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan (Kali host)"; flow:to_server; flags:S,12; window: 1024; tcp.mss: 1460; priority:1; classtype:attempted-recon; sid:1010021; rev:2;)
# -sF (FIN scan) (open|filtered)) | VM (open|filtered)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sF (FIN scan)"; flow:to_server; flags:F,12; window: 1024; priority:1; classtype:attempted-recon; sid:1010022; rev:2;)
# -sN (Null scan) (open|filtered) | VM (open|filtered)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sN (Null scan)"; flow:to_server; flags:0,12; window: 1024; priority:1; classtype:attempted-recon; sid:1010023; rev:2;)
# -sX (XMas scan) (open|filtered) | VM (open|filtered)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sX (XMas scan)"; flow:to_server; flags:FPU,12; window: 1024; priority:1; classtype:attempted-recon; sid:1010024; rev:2;)
# -sM (Maimon scan) (open|filtered) | VM (closed)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sM (Maimon scan)"; flow:to_server; flags:FA,12; window: 1024; priority:1; classtype:attempted-recon; sid:1010025; rev:2;)
# -sA & -sW (ACK & Window scan) (filtered) | VM (-sA unfiltered   -sW open)
drop tcp any any -> $HOME_NET any (msg:"SCAN Potential Ports Scan -sA/-sW (ACK/Window scan)"; flow:to_server; flags:A,12; window: 1024; priority:1; classtype:attempted-recon; sid:1010026; rev:4;)

# IP Reputation - Blacklisted Hosts
drop ip $HOME_NET any -> any any (msg:"IP Reputation - Blacklisted"; iprep:any,blacklist,>,9; priority:1; classtype:policy-violation; sid:1010030; rev:3;)
drop ip $EXTERNAL_NET any -> $HOME_NET any (msg:"TOR Exit Nodes - Blacklisted"; iprep:src,torexitnode,>,9; priority:1; classtype:policy-violation; sid:1010031; rev:3;)

